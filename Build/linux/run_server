#!/bin/bash

cd ./AllianceServer
./AllianceServer -d -nAllianceServer1
sleep 1
./AllianceServer -d -nAllianceServer2
sleep 1
./AllianceServer -d -nAllianceServer3
sleep 1
cd ..
echo "Start AllianceServer1 Ok!"

cd ./AllianceServerC
./AllianceServerC -d -nAllianceServerC
sleep 1
cd ..
echo "Start AllianceServerC Ok!"

cd ./FriendServer
./FriendServer -d -nFriendServer1
sleep 1
./FriendServer -d -nFriendServer2
sleep 1
./FriendServer -d -nFriendServer3
sleep 1
cd ..
echo "Start FriendServer1 Ok!"

cd ./FriendServerC
./FriendServerC -d -nFriendServerC
sleep 1
cd ..
echo "Start FriendServerC Ok!"

cd ./GameServer
./GameServer -d -nGameServer1
sleep 1
./GameServer -d -nGameServer2
sleep 1
./GameServer -d -nGameServer3
sleep 1
cd ..
echo "Start GameServer1 Ok!"

cd ./TransferServer
./TransferServer -d -nTransferServer
sleep 1
cd ..
echo "Start TransferServer Ok!"

cd ./WorldServer
./WorldServer -d -nWorldServer1
sleep 1
./WorldServer -d -nWorldServer2
sleep 1
./WorldServer -d -nWorldServer3
sleep 1
cd ..
echo "Start WorldServer Ok!"

cd ./RegServer
./RegServer -d -nRegServer
sleep 1
cd ..
echo "Start RegServer Ok!"

cd ./LoginServer
./LoginServer -d -nLoginServer
sleep 1
cd ..
echo "Start LoginServer Ok!"


